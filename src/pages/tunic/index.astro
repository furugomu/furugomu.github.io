---
import SiteLayout from "../../layouts/SiteLayout.astro";
import p48 from "./_assets/48.png";
import p11 from "./_assets/11.png";
import p02 from "./_assets/02.png";
import p06 from "./_assets/06.png";
import p52 from "./_assets/52.png";
import p27 from "./_assets/27.png";
import p21 from "./_assets/21.png";
import p04 from "./_assets/04.png";
import p50 from "./_assets/50.png";
import p42 from "./_assets/42.png";
import p15 from "./_assets/15.png";
import p22 from "./_assets/22.png";
import p55 from "./_assets/55.png";
import p31 from "./_assets/31.png";
import p33 from "./_assets/33.png";
import p46 from "./_assets/46.png";
import p28 from "./_assets/28.png";
import p09 from "./_assets/09.png";
import p40 from "./_assets/40.png";
import p18 from "./_assets/18.png";
import p34 from "./_assets/34.png";
import p16 from "./_assets/16.png";
import p12 from "./_assets/12.png";
import p39 from "./_assets/39.png";
import p44 from "./_assets/44.png";
const images = [
  [p48, p11, p02, p06, p52],
  [p27, p21, p04, p50, p42],
  [p15, p22, p55, p31, p33],
  [p46, p28, p09, p40, p18],
  [p34, p16, p12, p39, p44],
];
---

<SiteLayout title="Tunic">
  {
    images.map((row, i) => (
      <div class="row">
        {row.map((image, j) => (
          <img src={image.src} alt="map" />
        ))}
      </div>
    ))
  }
</SiteLayout>
<style>
  img {
    outline: solid 1px gold;
  }
</style>
